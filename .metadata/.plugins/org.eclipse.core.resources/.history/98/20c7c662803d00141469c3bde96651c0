package be.stijn.intranet.webservice;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class OutputDomain {
	private long id;
	private String name;
	private int word;
	private int bit;
	private int nr;
	private boolean value;
	
	public long getId(){
		return id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public int getWord() {
		return word;
	}
	
	public void setWord(int word) {
		this.word = word;
	}
	
	public int getBit() {
		return bit;
	}
	
	public void setBit(int bit) {
		this.bit = bit;
	}
	
	public int getNr() {
		return nr;
	}
	
	public void setNr(int nr) {
		this.nr = nr;
	}
	
	public boolean getValue() {
		return value;
	}
	
	public void setValue(boolean value) {
		this.value = value;
	}
}
